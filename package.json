        "@types/gulp": "^4.0.9",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.0.0",
        "@types/request": "^2.48.4",
        "@types/vscode": "1.63.0",
        "@types/websocket": "^1.0.5",
        "@typescript-eslint/eslint-plugin": "^4.28.3",
        "eslint": "^7.19.0",
        "eslint-plugin-import": "^2.22.1",
        "glob": "^7.1.6",
        "gulp": "^4.0.2",
        "mocha": "^10.0.0",
        "mocha-junit-reporter": "^2.0.0",
        "mocha-multi-reporters": "^1.5.1",
        "ts-node": "^7.0.1",
        "typescript": "^4.3.5",
        "vsce": "^2.11.0",
        "vscode-test": "^1.5.2",
        "webpack": "^5.28.0",
        "webpack-cli": "^4.6.0"
    },
    "dependencies": {
        "@azure/arm-appplatform": "^2.1.0-beta.3",
        "@azure/core-client": "^1.6.1",
        "@azure/ms-rest-js": "^2.6.1",
        "@azure/storage-file-share": "^12.3.0",
        "@microsoft/vscode-azext-azureutils": "^0.1.3",
        "@microsoft/vscode-azext-utils": "^0.1.1",
        "fs-extra": "^8.1.0",
        "request": "^2.88.2",
        "vscode-extension-telemetry-wrapper": "^0.8.0",
        "vscode-nls": "^4.1.1",
        "websocket": "^1.0.34"
    },
    "extensionDependencies": [
        "ms-vscode.azure-account"
    ],
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    }
